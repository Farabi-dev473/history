{
  "name": "moonly-bot",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "prisma:studio": "prisma studio",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "prisma:seed": "prisma db seed",
    "prisma:deploy": "yarn prisma:migrate && yarn prisma:generate",
    "serve": "concurrently \"npm:start:*\" --restart-tries -1",
    "start:back-end": "node ./dist/apps/back-end/main.js",
    "start:front-end": "cd ./dist/apps/front-end && yarn start",
    "dev:front-end": "nx serve front-end --port=3000",
    "dev:back-end": "nx serve back-end",
    "build": "yarn prisma:deploy && nx run-many --target=build --all --prod",
    "dev": "concurrently \"npm:dev:*\"",
    "db:up": "docker-compose up -d",
    "db:down": "docker-compose down",
    "test:watch": "nx test --watch",
    "test": "nx test",
    "deploy": "zx tools/deploy.mjs",
    "tmux": "tools/tmux-monitor.sh",
    "restart": "pm2 restart ecosystem.config.js"
  },
  "private": true,
  "prisma": {
    "seed": "node prisma/seed.js"
  },
